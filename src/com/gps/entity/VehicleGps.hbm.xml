<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- 1 配置类和表对应 
		class标签
		name属性：实体类全路径
		table属性：数据库表名称
	-->
	<class name="com.gps.entity.VehicleGps" table="vehiclegps">
		<id name="gpsId" column="gpsId">
			<generator class="native"></generator>
		</id>
		<property name="gpsCode"></property>
		<property name="gpsTime"></property>
		<property name="gpsLng"></property>
		<property name="gpsLat"></property>
		<property name="gpsDiming"></property>
		
	</class>
</hibernate-mapping>





